package com.arp.sampleMethods;

import java.io.File;

public class BasicsMethods_3 {

	public static void main(String[] args) {

		File file = new File("E:/examples/data.txt");

		// rename a file:-to rename a file we should pass the File object as a parameter to rename()
		File file1 = new File("E:/examples/file.txt");
		System.out.println(file.renameTo(file1));
	}

}
